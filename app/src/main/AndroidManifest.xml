        <activity android:name=".LocationActivity"/>
        <activity android:name=".ChangeProfileActivity"/>
        <activity android:name=".PackageActivity"/>
        <activity android:name=".PaymentMethodActivity"/>
        <activity android:name=".TransactionActivity"/>
        <activity android:name=".QRcodeActivity"/>
        <activity android:name=".CustomCaptureActivity"
